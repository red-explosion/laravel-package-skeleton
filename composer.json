{
    "name": "red-explosion/skeleton-laravel",
    "description": "A skeleton for building Laravel packages.",
    "license": "MIT",
    "homepage": "https://github.com/red-explosion",
    "type": "library",
    "keywords": [
        "red-explosion",
        "laravel"
    ],
    "authors": [
        {
            "name": "Ben Sherred",
            "email": "ben@sherred.co.uk"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "red-explosion/pint-config": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "RedExplosion\\SkeletonLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedExplosion\\SkeletonLaravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/pint --config vendor/red-explosion/pint-config/pint.json",
        "test:lint": "./vendor/bin/pint --config vendor/red-explosion/pint-config/pint.json --test",
        "test": [
            "@test:lint"
        ]
    },
    "extra": {},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
